{
  "name": "clomonitor",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "apexcharts": "3.49.0",
    "classnames": "^2.5.1",
    "clo-ui": "https://github.com/cncf/clo-ui.git#v0.5.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nth-check": "^2.0.1",
    "react": "^18.3.1",
    "react-apexcharts": "^1.5.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^9.1.0",
    "react-router-dom": "^6.30.0",
    "react-syntax-highlighter": "^15.6.6",
    "rehype-external-links": "^3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@eslint/js": "^9.36.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.5.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "jsdom": "^25.0.1",
    "jest-fetch-mock": "^3.0.3",
    "minimatch": "9.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "shx": "^0.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.12",
    "vitest": "^4.0.4"
  },
  "resolutions": {
    "@types/react-syntax-highlighter/**/@types/react": "18.3.18",
    "@types/react-router-dom/**/@types/react": "18.3.18",
    "@types/react-router-dom/**/@types/react-dom": "18.3.5",
    "clo-ui/**/rimraf": "^3.0.2"
  },
  "scripts": {
    "copy:static": "shx rm -rf src/static && shx mkdir src/static && shx cp -r public/static/* src",
    "copy:scorecard": "shx cp public/scorecard.html build",
    "build": "yarn copy:static && vite build && shx cp -r build/static/* build && shx rm -rf build/static && yarn copy:scorecard",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:diff": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "isready": "yarn format && yarn lint && yarn test --passWithNoTests && yarn build",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "preview": "vite preview",
    "start": "yarn copy:static && vite --host",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --passWithNoTests --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
